#!/usr/bin/env ruby

require_relative '../lib/move.rb'

puts "Welcome to Tic Tac Toe!"
board = [" "," "," "," "," "," "," "," "," "]
display_board(board)
move(board,5)
puts "Where would you like to go?"
input = gets.strip
input_to_index(input)
puts input
move(board,input)
puts "move worked"
display_board(board)